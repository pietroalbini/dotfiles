{
    "coc.source.around.enable": false,
    "coc.source.buffer.enable": "false,
    "coc.source.file.enable": "false,

    // The first tab should select the first element, the first element shouldn't be pre-selected.
    "suggest.noselect": true,

    // Disable function signature and documentation on hover
    "signature.target": "echo",
    "hover.target": "echo",

    "diagnostic.errorSign": "E: ",
    "diagnostic.infoSign": "I: ",
    "diagnostic.warningSign": "W: ",
    "diagnostic.signLevel": "information",

    "rust-analyzer.imports.granularity.group": "module",
    "rust-analyzer.imports.group.enable": false,
    "rust-analyzer.imports.prefix": "crate",
    "rust-analyzer.rustcSource": "discover"
}
